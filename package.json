{
    "devDependencies": {
        "@types/bootstrap": "^5.2.10",
        "@types/jest": "^29.5.12",
        "@types/jquery": "^3.5.16",
        "@types/sortablejs": "^1.3.32",
        "@eslint/js": "^9.20.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-jasmine2": "^29.7.0",
        "jest-ts-webcompat-resolver": "^1.0.0",
        "prettier": "^3.5.1",
        "sass": "1.77.1",
        "ts-jest": "^29.2.0",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.24.1"
    },
    "jest": {
        "testRunner": "jest-jasmine2",
        "rootDir": "evap/static/ts",
        "testMatch": [
            "**/tests/unit/**/*.ts",
            "**/tests/frontend/**/*.ts"
        ],
        "moduleDirectories": [
            "node_modules",
            "evap/static/ts"
        ],
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "resolver": "jest-ts-webcompat-resolver"
    },
    "type": "module"
}
